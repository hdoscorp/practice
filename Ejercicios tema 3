public class Principal {
    public static void main(String[] args) {
        int param1 = 10;          
        int param2 = 30; 
        int param3 = 20;  

        var resultado = suma(param1, param2, param3);

        System.out.println(resultado);
    }

    public static int suma(int a, int b, int c) {
        int resultado;
        resultado = a + b + c;
        System.out.println(resultado);
    }

}
